<resources>

    <!--百度地图点聚合-->
    <style name="ClusterIcon.TextAppearance" parent="android:TextAppearance">
        <item name="android:textColor">#ffeeeeee</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Bubble.TextAppearance.Dark" parent="android:TextAppearance.Medium">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#ff7f7f7f</item>
    </style>

    <style name="Bubble.TextAppearance.Light" parent="Bubble.TextAppearance.Dark">
        <item name="android:textColor">#ffeeeeee</item>
    </style>

    <style name="base_divider_line">
        <item name="android:background">#eeeeee</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
    </style>

    <style name="main_menu_photo_anim">
        <item name="android:windowEnterAnimation">@anim/dialog_in_anim</item>
        <item name="android:windowExitAnimation">@anim/dialog_out_anim</item>
    </style>

    <!--multiline_radiobutton-->
    <style name="multiline_radiobutton">
        <item name="android:button">@null</item>
        <item name="android:background">@drawable/multiline_radiobutton_background_selector</item>
        <item name="android:textColor">@drawable/multiline_radiobutton_text_color_selector</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">@dimen/rb_padding_top_bottom</item>
        <item name="android:paddingBottom">@dimen/rb_padding_top_bottom</item>
        <item name="android:layout_marginTop">@dimen/btn_marginTop</item>
        <item name="android:layout_marginBottom">@dimen/btn_marginTop</item>
        <item name="android:layout_marginLeft">@dimen/btn_marginLeft</item>
        <item name="android:layout_marginRight">@dimen/btn_marginLeft</item>
    </style>

    <!--树形View-->
    <style name="TreeNodeStyleCustom">
        <item name="android:paddingLeft">10dp</item>
        <!--<item name="android:showDividers">middle</item>-->
        <!--<item name="android:divider">@android:drawable/divider_horizontal_bright</item>-->
    </style>

    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
    </style>


    <style name="Index_Disaster_Num">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/dp_10</item>
        <item name="android:textSize">@dimen/sp_20</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="Index_Disaster_Label">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/dp_10</item>
        <item name="android:layout_marginBottom">@dimen/dp_20</item>
        <item name="android:textSize">@dimen/sp_14</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="MyTablayoutstyle">
        <item name="tabBackground">@color/white</item>
        <item name="tabIndicatorColor">@color/red</item>
        <item name="tabIndicatorHeight">2dp</item>
        <item name="tabSelectedTextColor">@color/red</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/red</item>
        <item name="tabTextAppearance">@dimen/sp_14</item>
        <item name="tabGravity">fill</item>
    </style>

    <style name="Disaster_Detail_Text_Label">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1.2</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">@dimen/dp_5</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/shape_corner_tv_bg</item>
        <item name="android:textSize">@dimen/sp_14</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="Disaster_Detail_Edit_Text">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">2.8</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">@dimen/dp_5</item>
        <item name="android:paddingLeft">@dimen/dp_5</item>
        <item name="android:gravity">left|center</item>
        <item name="android:background">@drawable/shape_corner_et_bg</item>
        <item name="android:textSize">@dimen/sp_14</item>
    </style>

    <!--顶部Layout布局-->
    <style name="CornerUpperLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_50</item>
        <item name="android:background">@drawable/shape_upper_corner</item>
        <item name="android:layout_marginLeft">@dimen/dp_5</item>
        <item name="android:layout_marginRight">@dimen/dp_5</item>
        <item name="android:layout_marginTop">@dimen/dp_10</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <!--中间样式-->
    <!--该样式在style下识别不出来-->
    <!--<item name="android:background">#ffffff</item>-->
    <style name="CornerMiddleLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_40</item>
        <item name="android:background">@drawable/shape_middle_corner</item>
        <item name="android:layout_marginLeft">@dimen/dp_5</item>
        <item name="android:layout_marginRight">@dimen/dp_5</item>
        <item name="android:layout_marginTop">@dimen/dp_3</item>
    </style>

    <!--底部样式-->
    <style name="CornerBottomLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_50</item>
        <item name="android:background">@drawable/shape_bottom_corner</item>
        <item name="android:layout_marginLeft">@dimen/dp_5</item>
        <item name="android:layout_marginRight">@dimen/dp_5</item>
        <item name="android:layout_marginTop">@dimen/dp_3</item>
    </style>

    <style name="Fire_Summary_Label">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1.2</item>
        <item name="android:gravity">center</item>
        <item name="android:background">#D5D5D5</item>
        <item name="android:textSize">@dimen/sp_12</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="Detail_TextView_Label">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1.2</item>
        <item name="android:gravity">center</item>
        <item name="android:background">#CEE5F7</item>
        <item name="android:textSize">@dimen/sp_12</item>
        <item name="android:textColor">#000000</item>
    </style>


    <style name="Fire_Summary_Content">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">2.8</item>
        <item name="android:paddingLeft">@dimen/dp_10</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/shape_corner_et_bg</item>
        <item name="android:textSize">@dimen/sp_12</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="Detail_EditText_Content">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">2.8</item>
        <item name="android:paddingLeft">@dimen/dp_10</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/shape_corner_et_bg</item>
        <item name="android:textSize">@dimen/sp_12</item>
        <item name="android:textColor">#000000</item>
    </style>

    <!--设置Tablayout字体大小-->
    <style name="TabLayoutTextSize">
        <item name="android:textSize">15sp</item>
    </style>


    <!--以下是腾讯云实时音视频要使用-->
    <style name="progressBarHorizontal_indeter" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:minHeight">3dp</item>
        <item name="android:maxHeight">3dp</item>
    </style>

    <style name="room_more_dlg" parent="@android:style/Theme.Holo.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>

    <style name="room_setting_dlg" parent="@android:style/Theme.Holo.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>

    <!--更新数字进度条-->
    <style name="UpdateAppNumberProgressBar_Red">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>

        <item name="custom_progress_max">100</item>
        <item name="custom_progress_current">0</item>

        <item name="custom_progress_unreached_color">#CCCCCC</item>
        <item name="custom_progress_reached_color">#e94339</item>

        <item name="custom_progress_text_size">12sp</item>
        <item name="custom_progress_text_color">#e94339</item>

        <item name="custom_progress_reached_bar_height">4dp</item>
        <item name="custom_progress_unreached_bar_height">3dp</item>
    </style>

    <style name="UpdateAppDialog" parent="android:style/Theme.Dialog">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowCloseOnTouchOutside">false</item>
        <item name="android:windowEnterAnimation">@anim/update_app_window_in</item>
        <item name="android:windowExitAnimation">@anim/update_app_window_out</item>
    </style>

    <style name="Chemical_detail_Content">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">2.4</item>
        <item name="android:paddingLeft">@dimen/dp_10</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/sp_14</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="Chemical_detail_Label">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1.2</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:paddingLeft">@dimen/dp_10</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:textSize">@dimen/sp_14</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="Chemical_Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:background">@color/dark_gray</item>
        <item name="android:layout_marginStart">@dimen/dp_10</item>
        <item name="android:layout_marginEnd">@dimen/dp_10</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

</resources>
