<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--点击特效：当用户按下[按钮]时的特效-->
    <item android:state_pressed="true">
        <shape>
            <corners android:bottomLeftRadius="5dp" android:bottomRightRadius="5dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />
            <solid android:color="@color/divider_list" />
            <stroke android:width="1dp" android:color="@color/divide_line" />
        </shape>
    </item>
    <!--点击特效：当用户没按下[按钮]时的特效-->
    <item android:state_pressed="false">
        <shape>
            <corners android:bottomLeftRadius="5dp" android:bottomRightRadius="5dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />
            <solid android:color="@color/white" />
            <stroke android:width="1dp" android:color="@color/divide_line" />
        </shape>
    </item>
</selector><!--<?xml version="1.0" encoding="utf-8"?>--><!--<selector xmlns:android="http://schemas.android.com/apk/res/android">--><!--&lt;!&ndash;初始状态&ndash;&gt;--><!--<item android:drawable="@drawable/right_normal" android:state_pressed="false" />--><!--&lt;!&ndash;按下状态&ndash;&gt;--><!--<item android:drawable="@drawable/right_selected" android:state_pressed="true" />-->

